{
  "name": "dr-ui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href=/dr-ui/ --configuration production --build-optimizer",
    "watch": "ng build --watch --configuration development",
    "test:coverage": "ng test --code-coverage",
    "test": "ng test",
    "mock": "node ./mock-server-src/mock-server",
    "nuke": "rm -rf node_modules && rm -rf build",
    "setup": "npm install --no-optional --legacy-peer-deps;",
    "lint": "ng lint",
    "copyAppDepDescription": "node copyAppDepDescription.js ./dist/dr-ui/assets/app-dep.json",
    "authenticate-erad": "sh ./scripts/authenticate-erad.sh",
    "setup-eds": "npm config set @eds:registry https://arm.rnd.ki.sw.ericsson.se/artifactory/api/npm/proj-eds-npm-local",
    "setup-uisdk": "npm config set @uisdk:registry=https://arm.seli.gic.ericsson.se/artifactory/api/npm/proj-uisdk-npm-local/",
    "setup-erad": "npm config set @erad:registry https://arm.seli.gic.ericsson.se/artifactory/api/npm/proj-erad-npm-local/",
    "setup-euisdk": "npm config set @eui:registry=https://arm.seli.gic.ericsson.se/artifactory/api/npm/proj-e-uisdk-npm-local/ && npm config set registry=https://arm.seli.gic.ericsson.se/artifactory/api/npm/proj-e-uisdk-npm/",
    "test-headless": "ng test --browsers=ChromeHeadlessNoSandbox --no-watch --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.1.0",
    "@angular/cdk": "12.1.0",
    "@angular/common": "12.1.0",
    "@angular/compiler": "12.1.0",
    "@angular/core": "12.1.0",
    "@angular/forms": "12.1.0",
    "@angular/material": "12.1.0",
    "@angular/material-moment-adapter": "12.1.0",
    "@angular/platform-browser": "12.1.0",
    "@angular/platform-browser-dynamic": "12.1.0",
    "@angular/router": "12.1.0",
    "@erad/app-switcher": "~0.2.0",
    "@erad/assets": "^0.2.25",
    "@erad/components": "0.2.183",
    "@erad/core": "~0.2.0",
    "@erad/customization": "~0.2.0",
    "@erad/jxc": "~0.2.0",
    "@erad/schematics": "~0.2.0",
    "@erad/smart-components": "~0.2.0",
    "@erad/utils": "~0.2.15",
    "@eui/base": "1.7.8",
    "@eui/component": "1.0.4",
    "@eui/lit-component": "1.1.1",
    "@eui/table": "1.3.12",
    "@eui/theme": "1.1.8",
    "@ngrx/effects": "12.1.0",
    "@ngrx/entity": "12.1.0",
    "@ngrx/router-store": "12.1.0",
    "@ngrx/store": "12.1.0",
    "@ngrx/store-devtools": "12.1.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@webcomponents/scoped-custom-element-registry": "0.0.6",
    "angular-resizable-element": "3.3.5",
    "cookie-parser": "^1.4.5",
    "dexie": "2.0.4",
    "material-design-icons-iconfont": "3.0.3",
    "moment": "2.24.0",
    "ngx-infinite-scroll": "9.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "~12.1.0",
    "@ngrx/schematics": "^12.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "dotenv": "^16.0.3",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "rxjs-marbles": "^7.0.1",
    "typescript": "~4.3.2"
  }
}
